<Window x:Class="ParfumerApp.Views.Windows.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ParfumerApp.Views.Windows"
        mc:Ignorable="d"
        Title="MainWindow"  MaxHeight="550" MinHeight="450" MaxWidth="1000" MinWidth="800" Height="520" Width="900">
    <Grid  Background="#FFB3F4E9">
        <Grid.RowDefinitions>
            <RowDefinition Height="20"/>
            <RowDefinition/>

        </Grid.RowDefinitions>
        <StackPanel Margin="0,0,10,0" Orientation="Horizontal" HorizontalAlignment="Right" VerticalAlignment="Center">
        <TextBlock Name="tlbUserName" FontSize="15"></TextBlock>
            
        </StackPanel>
        <Frame Grid.Row="1" Name="MainFrame" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
